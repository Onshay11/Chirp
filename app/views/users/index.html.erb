<div class="header">
  <div class="container">
    <h1>Log in Page</h1>
  </div>
</div>

<% @users.each { |u| %>
    <h3> User: <%= link_to u.name, user_path(u) %> </h3>
    <p> Account Type: <%=u.provider %></p>
    <% if u.provider == "created" %>
      <p> Email: <%= u.email %></p>
      <%end %>
    <p> Created: <%= u.created_at %></p>
    <% } %>
